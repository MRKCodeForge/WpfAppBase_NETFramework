<Window x:Class="WpfAppBaseDotNetFramework.Views.SecondaryWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfAppBaseDotNetFramework.Views"
        xmlns:vm="clr-namespace:WpfAppBaseDotNetFramework.ViewModels"
        mc:Ignorable="d"
        d:DataContext="{d:DesignInstance vm:SecondaryWindow_VM, IsDesignTimeCreatable=False}"
        Title="SecondaryWindow"
        Height="450"
        Width="800"
        DataContext="{Binding Source={StaticResource Locator}, Path=SecondaryWindow_VM}">
    <Grid>
        <TextBlock VerticalAlignment="Center"
                   HorizontalAlignment="Center"
                   Text="{Binding Message}" />
    </Grid>
</Window>
